

  <link rel="stylesheet" href="{{ asset('bundles/framework/css/myEntities.css') }}" />

	<h1>My View</h1>



	<table>
		<tr>
			<th>id</th>
			<th>lastName</th>
			<th>firtsName</th>
			<th>middleName</th>
			<th>birthDate</th>
			<th>address</th>
			<th>telNumber</th>
			<th>gender</th>
			<th>dateEmployed</th>
			<th>salary</th>
			<th colspan="3"><center>Action</center></th>
		</tr>
		{% for key in empInfo %}
		<tr>
			<td>{{key.id}}</td>
			<td>{{key.lastName}}</td>
			<td>{{key.firstName}}</td>
			<td>{{key.middleName}}</td>
			<td>{{key.birthDate}}</td>
			<td>{{key.address}}</td>
			<td>{{key.telNumber}}</td>
			<td>{{key.gender}}</td>
			<td>{{key.dateEmployed}}</td>
			<td>{{key.salary}}</td>
			<td><button><a href="{{ absolute_url(asset('')) }}app_dev.php/viewthis/{{ key.id }}">View</a></button></td>
			<td><button><a href="{{ absolute_url(asset('')) }}app_dev.php/edit/{{ key.id }}">Edit</a></button></td>
			<td><button><a href="{{ absolute_url(asset('')) }}app_dev.php/delete/{{ key.id }}">Delete</a></button></td>
		</tr>
		{% endfor %}
	</table>



